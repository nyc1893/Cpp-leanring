#include<iostream>
using namespace std;
class person
{
private:
    int m_age;
public:

    virtual void say();


};

class jack: public person
{
private:
    int m_age;
public:
        void say();
};

void person::say(){
    cout<<"I can say a word"<<endl;
}

void jack::say(){
    cout<<"I can say a sentence"<<endl;
}


int main()
{
    person *p1 = new jack;
    p1 -> say();
    delect p1;
    return 0;
}
